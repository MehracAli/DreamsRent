            </div>
        </div>
    </section>
    <section id="explore">
        <div class="container">
            <div data-aos="fade-down" class="explore-title">
                <h2>Explore Most Popular Cars</h2>
                <img src="~/assets/images/footer/title-head.png" alt="">
                <p>Lorem Ipsum has been the industry's standard dummy text ever since the 1500s</p>
            </div>
            <div data-aos="fade-down" class="explore-body-top">
                <ul style="justify-content: center">
                    @foreach (Brand brand in brands)
                    {
                        <li style="margin-right:20px">
                            <a href="#">
                                <span style="padding: 5px;width:50px;height:50px;">
                                    <img style="width:100%;" src="~/assets/images/index/brands/@brand.BrandLogo" alt="">
                                </span>
                                @brand.Name
                            </a>
                        </li>
                    }
                </ul>
            </div>
            <div class="explore-body-bottom">
                <div class="cars">
                    <div class="row">
                        @foreach (CarExploreVM car in cars)
                        {
                            <div data-aos="zoom-in-up" data-aos-duration="1000" class="car col-lg-4">
                            <div class="car-item">
                                <div class="car-img">
                                    <a asp-controller="Detail" asp-action="CarDetail" asp-route-Id="@car.Id">
                                        <img src="~/assets/images/cars/@car.CarPhotos.FirstOrDefault(p=>p.CarId == car.Id).Path" alt="">
                                    </a>
                                    <div class="fav">
                                        <span style="height:max-content">@car.Company.CompanyName</span>
                                        <a href="#" style="width:40px; height:40px">
                                            @if(likes.Any(l=>l.User.UserName == User.Identity.Name && l.CarId == car.Id))
                                            {
                                                <i class="fa-solid fa-heart" style="font-size:20px"></i>
                                            }
                                            else
                                            {
                                                <i class="fa-regular fa-heart" style="font-size:20px"></i>
                                            }
                                        </a>
                                    </div>
                                </div>
                                <div class="car-content">
                                    <div class="car-features">
                                        <a href="#" class="author-img" style="top:-45px">
                                            <img src="~/assets/images/users/@car.Company.CompanyPhoto"
                                                     style="width:60px;height:60px;padding:2px;border: 1px solid orange;background:white;" alt="">
                                        </a>
                                        <h3 class="car-title">
                                            <a href="#">
                                                    @car.Brand.Name @car.Brand.Models.FirstOrDefault(m=>m.Id == car.ModelId).Name
                                            </a>
                                        </h3>
                                        <div class="car-rating">
                                            @for (double i = 0; i < car.Rating; i++)
                                            {
                                                <i class="fa-solid fa-star"></i>
                                            }
                                            @for (double i = 0; i < 5 - car.Rating; i++)
                                            {
                                                <i class="fa-solid fa-star" style="color: gray;"></i>
                                            }
                                            <span>(@car.Rating)</span>
                                            <span style="position:absolute;right:5px;"> 
                                                <i class="fa-solid fa-heart"></i>
                                                Likes (@car.Likes.Count())
                                            </span>
                                        </div>
                                    </div>
                                    <div class="car-details">
                                        <ul>
                                            <li>
                                                <span>
                                                    <img src="~/assets/images/index/cars/car-detail/car-parts-01.svg"
                                                         alt="">
                                                </span>
                                                <p>@car.Transmission.Name</p>
                                            </li>
                                            <li>
                                                <span>
                                                    <img src="~/assets/images/index/cars/car-detail/car-parts-02.svg"
                                                         alt="">
                                                </span>
                                                <p>@car.Speed</p>
                                            </li>
                                            <li>
                                                <span>
                                                    <img src="~/assets/images/index/cars/car-detail/car-parts-03.svg"
                                                         alt="">
                                                </span>
                                                <p>@car.FuelType.Name</p>
                                            </li>
                                        </ul>
                                        <ul>
                                            <li>
                                                <span>
                                                    <img src="~/assets/images/index/cars/car-detail/car-parts-05.svg"
                                                         alt="">
                                                </span>
                                                <p>@car.Engine.HorsePower (hp)</p>
                                            </li>
                                            <li>
                                                <span>
                                                    <img src="~/assets/images/index/cars/car-detail/car-parts-04.svg"
                                                         alt="">
                                                </span>
                                                <p>@car.Year</p>
                                            </li>
                                            <li>
                                                <span>
                                                    <img src="~/assets/images/index/cars/car-detail/car-parts-06.svg"
                                                         alt="">
                                                </span>
                                                <p>@car.Capacity</p>
                                            </li>
                                        </ul>
                                    </div>
                                    <div class="car-location-price">
                                        <div class="car-location">
                                            <span>
                                                <i class="fa-solid fa-location-dot"></i>
                                                Germany
                                            </span>
                                        </div>
                                        <div class="car-price">
                                            <h6>
                                                $@car.Price
                                                <span>/Day</span>
                                            </h6>
                                        </div>
                                    </div>
                                    <div class="car-button">
                                        <a href="#">
                                            <span>
                                                <i class="fa-solid fa-calendar"></i>
                                            </span>
                                            Rent Now
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </section>
